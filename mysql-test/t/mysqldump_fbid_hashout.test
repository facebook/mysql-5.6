CREATE TABLE fbid (
  col0 bigint(20) NOT NULL DEFAULT '0',
  col1 int(11) NOT NULL DEFAULT '0',
  col2 int(11) NOT NULL DEFAULT '0',
  col3 varchar(255) NOT NULL DEFAULT '',
  col4 tinyint(4) DEFAULT '0',
  PRIMARY KEY (col0),
  KEY `type_id` (`col1`,`col0`)
) ENGINE = InnoDB;

