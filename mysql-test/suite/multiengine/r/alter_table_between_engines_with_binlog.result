CREATE TABLE t1 (pk INT PRIMARY KEY, a CHAR(30), b INT, KEY(a, b))
ENGINE=InnoDB COLLATE utf8_bin;
CREATE TABLE t2 LIKE t1;
INSERT INTO t2 VALUES(1, "1", 1);
FLUSH STATUS;
ALTER TABLE t2 ENGINE=RocksDB;
SELECT * FROM t2;
pk	a	b
1	1	1
DROP TABLE t1, t2;
