#
# bloom filter prefix is 20 byte
#  Create a key which is longer than that, so that we see that  
#       eq_cond_len= slice.size() - 1;
#  doesnt work.
#
#   indexnr   4
#   kp0     + 4 =  8
#   kp1     + 8 = 16
#   kp2     + 8 = 24    24>20 byte length prefix
#   kp3     + 8 = 28
create table t1 (
pk int primary key,
kp0 int not null,
kp1 bigint not null,
kp2 bigint not null,
kp3 bigint not null,
key kp12(kp0, kp1, kp2, kp3) comment 'rev:x1'
) engine=rocksdb;
